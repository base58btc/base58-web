  <script>
    const tabContainer = document.querySelector('.tab4_component');
    const tabMenu = tabContainer.querySelectorAll('.tab4_tab-link');
    let tabContent = tabContainer.querySelectorAll('.tab4_tab-pane');
    let tabRadioButtons = tabContainer.querySelectorAll('input[type="radio"]');
    tabRadioButtons.forEach((button, index) => {
      button.addEventListener('click', () => {
        tabContent.forEach(tabPane => {
          tabPane.classList.remove('active');
        })
        tabContent[index].classList.add('active');
        tabMenu.forEach(tabLink => {
          tabLink.classList.remove('active');
        })
        tabMenu[index].classList.add('active');
      })
    })
  </script>
