<!DOCTYPE html>
<html>
  <head>
    <title>Notion data</title>
  </head>
  <body>
    <table>
      <thead>
        <tr>
          <th>Name</th>
          <th>Duration</th>
          <th>Dates</th>
          <th>Times</th>
          <th>Price</th>
        </tr>
      </thead>
      <tbody>
        {{
          range.results
        }}
        <tr>
          <td>{{.properties.Name.title 0}}</td>
          <td>{{.properties.Duration.rich_text 0}}</td>
          <td>
            {{.properties.Dates.date.start}} - {{.properties.Dates.date.end}}
          </td>
          <td>{{.properties.Times.rich_text 0}}</td>
          <td>{{.properties.Price.rich_text 0}}</td>
        </tr>
        {{
          end
        }}
      </tbody>
    </table>
  </body>
</html>
